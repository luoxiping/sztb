<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/start_linear_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <include
        layout="@layout/topbar" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal" >
        <fragment
	        android:id="@+id/address_left"
	        android:name="com.sztb.socialsecurity.fragment.address.AddressLeftFragment"
	        android:layout_width="0dp"
	        android:layout_height="match_parent"
	        android:layout_weight="1.0" />
		<fragment
	        android:id="@+id/address_right"
	        android:name="com.sztb.socialsecurity.fragment.address.AddressRightFragment"
	        android:layout_width="0dp"
	        android:layout_height="match_parent"
	        android:layout_weight="2.0" />
    </LinearLayout>
    
</LinearLayout>
